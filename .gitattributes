# singularity containers
*.sif filter=lfs diff=lfs merge=lfs -tex

# plink files
*.fam filter=lfs diff=lfs merge=lfs -text
*.bim filter=lfs diff=lfs merge=lfs -text
*.bed filter=lfs diff=lfs merge=lfs -text
*.frq  filter=lfs diff=lfs merge=lfs -text

# general archives
*.bz2  filter=lfs diff=lfs merge=lfs -text
*.gz   filter=lfs diff=lfs merge=lfs -text
*.zip  filter=lfs diff=lfs merge=lfs -text
*.tar  filter=lfs diff=lfs merge=lfs -text

# matlab files
*.mat  filter=lfs diff=lfs merge=lfs -text

# specific files
reference/ldsc/w_hm3.snplist                 filter=lfs diff=lfs merge=lfs -text
reference/pleiofdr/9545380_ref/9545380.ref   filter=lfs diff=lfs merge=lfs -text

# mixer
*.log  filter=lfs diff=lfs merge=lfs -text
*.ld  filter=lfs diff=lfs merge=lfs -text
*.snps  filter=lfs diff=lfs merge=lfs -text

